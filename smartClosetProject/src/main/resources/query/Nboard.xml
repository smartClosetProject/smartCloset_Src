<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="package com.spring.admin.nboard.dao.NboardDAO">

	<!-- 게시판 리스트 기본 조회 -->
	
	<select id="nboardList" parameterType="nboard" resultType="nboard">
		SELECT n_num, n_author, n_title, TO_CHAR(n_writedate, 'YYYY-MM-DD') AS n_writedate
		FROM notice
		ORDER BY n_num desc
	</select> 
</mapper>